<ol class="post-list">
  {{#each posts as |post|}}
  <li>
    <article class="post">
      <div class="title">
        <a href="{{post_body}}" target="_blank">{{title}}</a>
      </div>
    <div class="postBody">
      {{post_body}}
    </div>
    </article>
  </li>
  {{/each}}
</ol>

